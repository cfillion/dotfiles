xterm*bellIsUrgent: true
xterm*borderWidth: 0
xterm*fastScroll: true
xterm*fullscreen: never
xterm*highlightSelection: true
xterm*internalBorder: 0
xterm*italicULMode: true
xterm*locale: true
xterm*metaSendsEscape: true
xterm*saveLines: 4096
xterm*scrollKey: true
xterm*scrollTtyOutput: false
xterm*termName: xterm-256color
xterm*title: 
xterm*utf8Title: true
xterm*utmpInhibit: true
xterm*waitForMap: true

! menu colors
xterm*foreground: #20ad20
xterm*background: #080808

! emulator colors
xterm*VT100*foreground: rgb:b2/b2/b2
xterm*VT100*background: rgb:00/00/00

! black
xterm*VT100*color0:  #000000
xterm*VT100*color8:  #3d3d3d
! red
xterm*VT100*color1:  #ff5d28
xterm*VT100*color9:  #ff5555
! green
xterm*VT100*color2:  #20ad20
xterm*VT100*color10: #5faf00
! yellow
xterm*VT100*color3:  #ffff00
xterm*VT100*color11: #e6ea50
! blue
xterm*VT100*color4:  #005f5f
xterm*VT100*color12: #268bd2
! magenta
xterm*VT100*color5:  #af5f5f
xterm*VT100*color13: #af8787
! cyan
xterm*VT100*color6:  #00afff
xterm*VT100*color14: #afd7ff
! white
xterm*VT100*color7:  #fefefe
xterm*VT100*color15: #ffffff

! font
xterm*faceName: DejaVu Sans Mono
xterm*faceNameDoublesize: HanaMinA
xterm*faceSize: 9

! selectable urls & custom shortcuts
xterm*charClass: 33:48,35:48,37-38:48,42-47:48,58:48,61:48,63-64:48,95:48,126:48
xterm*VT100*translations: #override \n\
  Ctrl Shift <Key> V: insert-selection(CLIPBOARD) \n\
  Mod4 <Key> Return: spawn-new-terminal() \n\
  Mod4 <Key> B: exec-formatted("dwb '%t'", PRIMARY)
