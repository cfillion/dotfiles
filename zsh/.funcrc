compress()
{
  convert -strip -interlace Plane -quality 85% $*
}

feh()
{
  opts = ''
  if [ "$1" ]; then
    opts = "$opts --start-at ./$1"
  fi

  command feh $opts
}

gem2ri()
{
  gem rdoc $* --ri --no-rdoc
}

lsfont()
{
  fc-list : family
}

myip()
{
  curl curlmyip.com
}

sprunge()
{
  curl -F 'sprunge=<-' http://sprunge.us
}

topdu()
{
  du -sh */ | sort -rh | less -NSFiX
}
